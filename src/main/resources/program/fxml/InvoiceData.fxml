<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1366.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Label text="Invoice Number:" />
            <TextField fx:id="invoiceNumberTextField" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#handleQueryInvoiceButtonAction" text="Query Invoice" />
      <Label text="Result" />
      <TextArea fx:id="resultTextArea" editable="false" prefHeight="400.0" prefWidth="200.0">
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </TextArea>
      <Button mnemonicParsing="false" onAction="#handleBackButtonAction" text="Back">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
